        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- ModelHasUseCases -->
  <xsd:element name="modelHasUseCases" abstract="true" type="ModelHasUseCases" substitutionGroup="core:modelElement" />
  <xsd:complexType name="ModelHasUseCases" abstract="true" >
    <xsd:annotation>
